file = {:home/lg/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Liu - 2009 - FastCluster a graph theory based algorithm for removing redundant sequences.pdf:pdf},
issn = {1937-6871},
journal = {Journal of Biomedical Science and Engineering},
keywords = {Hobohm Algorithm,Hobohm References,blast,graph theory,redundant sequences},
mendeley-tags = {Hobohm Algorithm,Hobohm References},
number = {08},
