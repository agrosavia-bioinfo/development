issn = {21601836},
journal = {G3: Genes, Genomes, Genetics},
keywords = {GWAS,GWAS Gene action,GWAS Mapping Models,GWAS Models,GWAS Poly,GWAS Population Structure,GWAS Potato,GWAS SNPs Evidence,Genome-wide association studies (GWAS),Linkage disequilibrium kinship,Mixed models,MultiGWAS Paper,Paper-MultiGWAS,Population structure,Potato,Single-nucleotide polymorphism (SNP),Tetraploid},
mendeley-tags = {GWAS,GWAS Gene action,GWAS Mapping Models,GWAS Models,GWAS Poly,GWAS Population Structure,GWAS Potato,GWAS SNPs Evidence,MultiGWAS Paper,Paper-MultiGWAS},
number = {10},
pages = {3185--3202},
