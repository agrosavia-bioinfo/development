mendeley-tags = {GWAS,GWAS Software,GWAS Tools,MultiGWAS Paper,Paper-MultiGWAS,Tassel},
number = {19},
pages = {2633--2635},
title = {{TASSEL: software for association mapping of complex traits in diverse samples}},
url = {https://doi.org/10.1093/bioinformatics/btm308},
volume = {23},
year = {2007}
}
