isbn = {1111111111},
issn = {1932-6203},
journal = {PLOS ONE},
keywords = {GWAS,GWAS Poly,GWAS Potato,Germplasm,MultiGWAS Paper,Paper-MultiGWAS},
mendeley-tags = {GWAS,GWAS Poly,GWAS Potato,Germplasm,MultiGWAS Paper,Paper-MultiGWAS},
month = {mar},
